{
  "name": "portfolio-cycle",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "cycle-scripts-one-fits-all": "^7.2.1",
    "cyclejs-test-helpers": "^4.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "snabbdom-looks-like": "^1.0.5",
    "snabbdom-pragma": "^2.8.0"
  },
  "scripts": {
    "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}' '*.{js,json,md,ts}'",
    "start": "PORT=8004 cycle-scripts start",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject",
    "clean": "cycle-scripts clean",
    "now-build": "yarn build && mv build dist"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "src/components"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "mocha-webpack": {
    "include": [
      "test/**/*.test.{js,jsx,ts,tsx}"
    ]
  },
  "dependencies": {
    "@cycle/dom": "^22.3.0",
    "@cycle/history": "^7.1.0",
    "@cycle/isolate": "^5.0.0",
    "@cycle/run": "^5.2.0",
    "@cycle/state": "^1.2.0",
    "@cycle/time": "^0.19.0",
    "cyclejs-utils": "^4.0.2",
    "cyclic-router": "^6.0.0",
    "switch-path": "^1.2.0",
    "xstream": "^11.10.0"
  },
  "one-fits-all": true,
  "resolutions": {
    "**/typescript": "3.1"
  }
}
